<rules>
  <rule name="redirect1" patternSyntax="Wildcard" stopProcessing="true">
    <match url="*" />
    <conditions>
      <add input="{URL}" pattern="*/media/contacts*" />
    </conditions>
    <action type="Redirect" url="/careers" />
  </rule>
